{
  "name": "discord-typescript-template",
  "version": "1.0.0",
  "description": "A template for a discord.js bot written in Typescript",
  "main": "bot.js",
  "scripts": {
    "build": "tsc",
    "runprod": "node /usr/src/app/src/bot.js",
    "rundev": "node /usr/src/app/src/bot.js",
    "rundevw": "nodemon /usr/src/app/src/bot.js",
    "lint": "npx eslint . --fix",
    "update": "npx npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.14.1",
    "enmap": "^5.9.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "npm-check": "^6.0.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
