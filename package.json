{
  "name": "discord-typescript-template",
  "version": "1.0.0",
  "description": "A template for a discord.js bot written in Typescript",
  "main": "bot.js",
  "scripts": {
    "build": "tsc",
    "runprod": "node /usr/src/app/src/bot.js",
    "rundev": "node /usr/src/app/src/bot.js",
    "rundevw": "nodemon /usr/src/app/src/bot.js",
    "lint": "npx eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "discord-api-types": "^0.37.20",
    "discord.js": "^14.6.0",
    "enmap": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}

