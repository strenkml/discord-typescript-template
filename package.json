{
  "name": "discord-typescript-template",
  "version": "1.0.0",
  "description": "A template for a discord.js bot written in Typescript",
  "main": "dist/bot.js",
  "scripts": {
    "build": "npx tsc",
    "start:prod": "node /usr/src/app/src/bot.js",
    "start:dev": "(npx tsc -w &) && npx nodemon",
    "lint": "npx eslint --ext .js,.ts .",
    "lintfix": "npx eslint . --fix",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenkml/discord-typescript-template.git"
  },
  "author": "Matt Strenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strenkml/discord-typescript-template/issues"
  },
  "homepage": "https://github.com/strenkml/discord-typescript-template#readme",
  "dependencies": {
    "discord.js": "^14.14.1",
    "enmap": "^5.9.8",
    "stumper": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "npm-check": "^6.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
